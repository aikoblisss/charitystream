{
  "version": 2,
  "builds": [
    {
      "src": "backend/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/backend/server.js"
    }
  ],
  "crons": [
    {
      "path": "/api/system/weekly-reset",
      "schedule": "59 23 * * SUN"
    }
  ]
}
